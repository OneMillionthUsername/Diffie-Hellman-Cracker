<Window x:Class="Diffie_Hellman_Cracker.File_Explorer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diffie_Hellman_Cracker"
        mc:Ignorable="d"
        Title="File Explorer">
    <DockPanel>
        <DataGrid x:Name="fileNames" DataGridRow.Selected="FileNames_Selected" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="15" HeadersVisibility="Column" IsReadOnly="True" MouseDoubleClick="Double_Click"/>
    </DockPanel>
</Window>
